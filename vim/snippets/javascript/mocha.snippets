# Override jasmine before each snippet
snippet bef "before all (mocha.js)" !b
before(function() {
	$0
});
endsnippet

# Override jasmine after each snippet
snippet aft "after all (mocha.js)" !b
after(function() {
	$0
});
endsnippet

snippet bfe "before each (mocha.js)" b
beforeEach(function() {
	$0
});
endsnippet

snippet afe "after each (mocha.js)" b
afterEach(function() {
	$0
});
endsnippet

snippet con "context (mocha.js)" b
context('${1:description}', function() {
	$0
});
endsnippet
